<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD donfig 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<!-- 마이바티스 작동 규칙 정의 -->
	<settings>
		<setting name="cacheEnabled" value="false"/>
		<setting name="useGeneratedKeys" value="false"/>
		<setting name="mapUnderscoreToCamelCase" value="true"/>	
	</settings>
	
	<typeAliases>
		<typeAlias alias="memberVO" type="com.group.exam.member.vo.MemberVO"/>
		<!-- 커맨드 객체 만들면 여기에 추가 -->
	</typeAliases>
</configuration>